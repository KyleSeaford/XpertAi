{
    "version": 2,
    "builds": [
      {
        "src": "XpertAi/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "XpertAi/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "XpertAi.settings",
      "PYTHONPATH": "."
    }
  }
  